{
  "name": "svg-to-url",
  "version": "2.0.0",
  "description": "A simple tool to convert svg file to data url",
  "repository": "zillding/svg-to-url",
  "bin": {
    "svg-to-url": "./bin/index.js"
  },
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prettier": "prettier --write '{bin,lib}/**/*.js'",
    "test": "jest",
    "release": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "svg",
    "data",
    "url"
  ],
  "author": "Zill Ding",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "clipboardy": "^2.1.0",
    "commander": "^4.0.0",
    "svgo": "^1.2.2",
    "update-notifier": "^3.0.0"
  },
  "devDependencies": {
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.0.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2"
  }
}
