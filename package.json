{
  "name": "svg-to-url",
  "version": "1.1.0",
  "description": "A simple tool to convert svg file to data url",
  "repository": "zillding/svg-to-url",
  "bin": {
    "svg-to-url": "./bin/index.js"
  },
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prettier": "prettier --write '{bin,lib}/**/*.js'",
    "precommit": "lint-staged",
    "test": "jest",
    "deploy": "np"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "svg",
    "data",
    "url"
  ],
  "author": "Zill Ding",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.0.1",
    "clipboardy": "^1.1.4",
    "commander": "^2.10.0",
    "svgo": "^0.7.2",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "husky": "^0.14.1",
    "jest": "^24.8.0",
    "lint-staged": "^4.0.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2"
  }
}
